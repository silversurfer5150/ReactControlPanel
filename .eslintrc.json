{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 0,
    "react/prop-types": 0,
    "max-len": [1, 150],
    "import/first": 0,
    "no-tabs": 0,
    "indent": ["warn"],
    "react/jsx-indent": ["warn"]
  }
}
